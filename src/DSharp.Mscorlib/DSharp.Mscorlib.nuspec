<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
  <metadata>
    <id>DSharp.Mscorlib</id>
    <title>DSharp Mscorlib</title>
    <authors>$authors$</authors>
    <version>$version$</version>
    <copyright>$copyright$</copyright>
    <description>Provides the default mscorlib library for all DSharp projects</description>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <developmentDependency>true</developmentDependency>
    
    <dependencies>
      <dependency id="DSharp.Scripts" version="$version$" include="contentFiles" />
    </dependencies>

    <contentFiles>
      <files include="any\net40\ss.meta.js" buildAction="Content" copyToOutput="true" />
      <files include="any\net471\ss.meta.js" buildAction="Content" copyToOutput="true" />
    </contentFiles>
  </metadata>
  <files>
    <file src="bin\$configuration$\net40\DSharp.Mscorlib.*" target="lib\net40" />
    <file src="bin\$configuration$\net471\DSharp.Mscorlib.*" target="lib\net471" />
    <file src="bin\$configuration$\net40\ss.meta.js" target="contentFiles\any\net40" />
    <file src="bin\$configuration$\net471\ss.meta.js" target="contentFiles\any\net471" />
  </files>
</package>