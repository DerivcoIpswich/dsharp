<Project>
  <Target Name="CopyScriptsToOutput" BeforeTargets="DispatchToInnerBuilds">
    <PropertyGroup>
      <ScriptsDistPath>$(SolutionDir)DSharp.Script\dist\</ScriptsDistPath>
    </PropertyGroup>
    <Copy SourceFiles="$(ScriptsDistPath)ss.js;$(ScriptsDistPath)ss.min.js" DestinationFolder="$(OutputPath)scripts\" SkipUnchangedFiles="true" />
  </Target>
</Project>